<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:background="@drawable/ellipse_3"
    tools:context=".ui.health.HealthFragment">


    <TextView
        android:id="@+id/text_health"
        android:layout_width="match_parent"
        android:layout_height="wrap_content"
        android:layout_marginBottom="133dp"
        android:textAlignment="center"
        android:textSize="20sp"
        app:layout_constraintBottom_toBottomOf="parent"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="0.0"
        app:layout_constraintStart_toStartOf="parent" />

    <ImageButton
        android:id="@+id/maternity"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"


        android:layout_marginTop="52dp"
        android:layout_marginBottom="275dp"
        android:src="@drawable/icon_maternity"
        android:text="Button"
        app:layout_constraintBottom_toTopOf="@+id/text_health"
        app:layout_constraintEnd_toStartOf="@+id/lady_0to6"
        app:layout_constraintHorizontal_bias="0.333"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/text1000goldendays" />

    <ImageButton
        android:id="@+id/lady6to12"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="18dp"
        android:layout_marginTop="40dp"
        android:layout_marginEnd="220dp"
        android:layout_marginBottom="1dp"
        android:src="@drawable/icon_6_12_months"
        android:text="Button"
        app:layout_constraintBottom_toTopOf="@+id/text1000goldendays"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toBottomOf="@+id/maternity"
        app:layout_constraintVertical_bias="0.0" />

    <ImageButton
        android:id="@+id/lady12to24"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="38dp"
        android:layout_marginTop="36dp"
        android:layout_marginEnd="16dp"
        android:layout_marginBottom="69dp"
        android:src="@drawable/icon_12_24_months"
        android:text="Button"
        app:layout_constraintBottom_toTopOf="@+id/text_health"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintHorizontal_bias="1.0"
        app:layout_constraintStart_toEndOf="@+id/lady6to12"
        app:layout_constraintTop_toBottomOf="@+id/lady_0to6" />

    <TextView
        android:id="@+id/text1000goldendays"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginTop="32dp"
        android:layout_marginBottom="34dp"
        android:text="1000 golden days"
        android:textColor="@color/white"
        android:textSize="32sp"
        app:layout_constraintBottom_toTopOf="@+id/lady_0to6"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toStartOf="parent"
        app:layout_constraintTop_toTopOf="parent" />


    <ImageButton
        android:id="@+id/lady_0to6"
        android:layout_width="wrap_content"
        android:layout_height="wrap_content"
        android:layout_marginStart="32dp"
        android:layout_marginTop="53dp"

        android:layout_marginEnd="16dp"
        android:layout_marginBottom="36dp"
        android:src="@drawable/icon_0_6_months"
        android:text="Button"
        app:layout_constraintBottom_toTopOf="@+id/lady12to24"
        app:layout_constraintEnd_toEndOf="parent"
        app:layout_constraintStart_toEndOf="@+id/maternity"
        app:layout_constraintTop_toBottomOf="@+id/text1000goldendays" />

</androidx.constraintlayout.widget.ConstraintLayout>